<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:background="@android:color/darker_gray"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


    <TextView
            android:text=""
            android:gravity="center"
            android:layout_width="100dp" android:layout_height="25dp"
            android:id="@+id/textView3" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toTopOf="parent"
    />
    <TextView
            android:text=""
            android:layout_width="100dp" android:layout_height="25dp"
            android:gravity="center"
            android:id="@+id/textView4" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="32dp"
            app:layout_constraintEnd_toStartOf="@+id/latiResult"
    />
    <TextView android:layout_width="100dp" android:layout_height="26dp"
              android:text=""
              android:gravity="center"
              android:textSize="18sp"
              android:id="@+id/longiResult"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="32dp" app:layout_constraintStart_toEndOf="@+id/textView3"
    />

    <TextView
            android:text=""
            android:layout_width="100dp"
            android:layout_height="26dp"
            android:gravity="center"
            android:textSize="18sp"
            android:id="@+id/latiResult"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toTopOf="parent"
    />

    <TextView
            android:text=""
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:id="@+id/moonriseResult"
            android:gravity="center"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
            app:layout_constraintTop_toBottomOf="@+id/longiResult"
            android:layout_marginTop="32dp"/>
    <TextView
            android:text=""
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:id="@+id/moonsetResult"
            android:gravity="center"
            android:textSize="18sp"
            app:layout_constraintEnd_toStartOf="@+id/imageView2"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/latiResult"/>
    <TextView
            android:text=""
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:id="@+id/newMoon"
            android:gravity="center"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/moonriseResult"/>
    <TextView
            android:text=""
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:id="@+id/fullMoon"
            android:gravity="center"
            android:textSize="18sp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/moonsetResult"
            app:layout_constraintEnd_toStartOf="@+id/imageView6" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp"/>
    <TextView
            android:text=""
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:id="@+id/phase"
            android:gravity="center"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="204dp" android:layout_marginStart="204dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView7" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/newMoon"/>
    <TextView
            android:text=""
            android:layout_width="230dp"
            android:layout_height="38dp"
            android:id="@+id/sinodicDay"
            android:gravity="center"
            android:textSize="30sp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/phase" app:layout_constraintHorizontal_bias="0.498"/>
    <ImageView
            android:layout_width="59dp"
            android:layout_height="40dp" app:srcCompat="@drawable/moonrise"
            android:id="@+id/imageView"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="90dp"
            app:layout_constraintStart_toEndOf="@+id/moonriseResult" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="59dp"
            android:layout_height="40dp" app:srcCompat="@drawable/moonset"
            android:id="@+id/imageView2"
            tools:ignore="MissingConstraints"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="32dp" android:layout_marginRight="32dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/latiResult"/>
    <ImageView
            android:layout_width="59dp"
            android:layout_height="40dp" app:srcCompat="@drawable/moonnew"
            android:id="@+id/imageView5"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView" app:layout_constraintStart_toEndOf="@+id/newMoon"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="59dp"
            android:layout_height="40dp"
            android:layout_marginEnd="32dp"
            android:layout_marginRight="32dp" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/imageView6"
            app:srcCompat="@drawable/fullmoon"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/imageView2"/>
    <ImageView
            android:layout_width="59dp"
            android:layout_height="40dp" app:srcCompat="@drawable/angle"
            android:id="@+id/imageView7"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toEndOf="@+id/phase"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="230dp" android:layout_marginRight="230dp" android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/fullMoon"/>

</androidx.constraintlayout.widget.ConstraintLayout>